Table user {
  user_id serial pk
}

Table user_space_role {
  user_space_role_id serial pk
  user_id integer
  space_id integer
  role_id integer
}

Table org_admin {
  org_admin_id serial pk
  user_id integer
  org_id integer
}

Table organization {
  org_id serial pk
  org_name varchar(50) Unique
}

Table role {
  role_id serial pk
  role_name varch(25)
}

Table role_permission {
  role_perm_id serial pk
  role_id integer
  perm_id integer
}

Table permission {
  perm_id serial pk
  perm_name varchar(25)
}

Table space {
  space_id serial pk
  space_name varchar(50)
  org_id intger
}

Table folder {
  folder_id serial pk
  folder_name varchar(25)
  space_id integer
}

Table page {
  page_id serial pk
  page_name varchar(25)
  folder_id integer
}

Ref: user.user_id < org_admin.user_id
Ref: user.user_id < user_space_role.user_id

Ref: user_space_role.role_id > role.role_id
Ref: user_space_role.space_id > space.space_id

Ref: org_admin.org_id > organization.org_id

Ref: space.org_id > organization.org_id

Ref: folder.space_id > space.space_id

Ref: page.folder_id > folder.folder_id

Ref: role_permission.role_id > role.role_id

Ref: role_permission.perm_id > permission.perm_id
